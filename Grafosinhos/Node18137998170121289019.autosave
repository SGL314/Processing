public class Node{
    Node father;
    int x,y,raw;
    Node (No father,int x,int y,int raw){
        this.father = father;
        this.x = x;
        this.y = y;
        this.raw = raw;
    }

    void drawNode(){
      fill(#FF0000);
      noStroke();
      circle(this.x,this.y,this.raw);
    }
}
